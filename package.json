{
  "name": "@goosebumps/token-lists",
  "version": "0.0.2",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "checksum:goosebumps-default": "yarn build && node ./dist checksum goosebumps-default",
    "generate:goosebumps-default": "yarn test && yarn build && node ./dist generate goosebumps-default",
    "makelist:goosebumps-default": "yarn checksum:goosebumps-default && yarn generate:goosebumps-default",
    "checksum:goosebumps-extended": "yarn build && node ./dist checksum goosebumps-extended",
    "generate:goosebumps-extended": "yarn test && yarn build && node ./dist generate goosebumps-extended",
    "makelist:goosebumps-extended": "yarn checksum:goosebumps-extended && yarn generate:goosebumps-extended",
    "checksum:goosebumps-top-100": "yarn build && node ./dist checksum goosebumps-top-100",
    "generate:goosebumps-top-100": "yarn test && yarn build && node ./dist generate goosebumps-top-100",
    "makelist:goosebumps-top-100": "yarn checksum:goosebumps-top-100 && yarn generate:goosebumps-top-100",
    "checksum:goosebumps-top-15": "yarn build && node ./dist checksum goosebumps-top-15",
    "generate:goosebumps-top-15": "yarn test && yarn build && node ./dist generate goosebumps-top-15",
    "makelist:goosebumps-top-15": "yarn checksum:goosebumps-top-15 && yarn generate:goosebumps-top-15",
    "checksum:coingecko": "yarn build && node ./dist checksum coingecko",
    "generate:coingecko": "yarn test && yarn build && node ./dist generate coingecko",
    "makelist:coingecko": "yarn checksum:coingecko && yarn generate:coingecko",
    "checksum:cmc": "yarn build && node ./dist checksum cmc",
    "generate:cmc": "yarn test && yarn build && node ./dist generate cmc",
    "makelist:cmc": "yarn checksum:cmc && yarn generate:cmc",
    "checksum:goosebumps-mini": "yarn build && node ./dist checksum goosebumps-mini",
    "generate:goosebumps-mini": "yarn test && yarn build && node ./dist generate goosebumps-mini",
    "makelist:goosebumps-mini": "yarn checksum:goosebumps-mini && yarn generate:goosebumps-mini",
    "checksum:goosebumps-mini-extended": "yarn build && node ./dist checksum goosebumps-mini-extended",
    "generate:goosebumps-mini-extended": "yarn test && yarn build && node ./dist generate goosebumps-mini-extended",
    "makelist:goosebumps-mini-extended": "yarn checksum:goosebumps-mini-extended && yarn generate:goosebumps-mini-extended",
    "from-addresses:goosebumps-mini-extended": "yarn build && node ./dist get-list-from-addresses",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "fetch:goosebumps-top-100": "yarn build && node ./dist fetch goosebumps-top-100",
    "fetch:coingecko": "yarn build && node ./dist fetch coingecko",
    "fetch:cmc": "yarn build && node ./dist fetch cmc",
    "test": "jest",
    "ci-check": "yarn build && node ./dist ci-check",
    "postinstall": "yarn run typechain",
    "typechain": "typechain --out-dir src/utils/abi/types --target=ethers-v5 \"src/utils/abi/*.json\""
  },
  "dependencies": {
    "@ethersproject/address": "^5.1.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@uniswap/token-lists": "^1.0.0-beta.21",
    "ajv": "6.12.2",
    "axios": "^0.26.0",
    "ethers": "^5.5.4",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.21",
    "slugify": "^1.6.0",
    "tslib": "^2.4.0",
    "typechain": "^7.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.182",
    "@types/node": "^15.0.2",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "rollup": "^2.47.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.3"
  }
}
